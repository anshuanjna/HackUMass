{
  "manifest_version": 3,
  "name": "TabSense",
  "version": "1.0",
  "description": "Organize your browser tabs by meaning and mood.",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "authors": "Rakshita Saroha , Pihul Nyati, Anshu Anjna, Prisha Babel"
}
